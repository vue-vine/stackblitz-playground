{
  "name": "vue-vine-playground",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "A vue-vine app created with create-vue-vine",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check only-build",
    "preview": "vite preview",
    "lint": "eslint src --cache",
    "lint:fix": "eslint src --cache --fix",
    "type-check": "vue-vine-tsc --noEmit --composite false",
    "only-build": "vite build"
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@types/node": "^20.17.6",
    "@vue-vine/eslint-config": "^0.2.13",
    "eslint": "^9.15.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3",
    "vite": "^6.3.5",
    "vue-vine": "^0.2.5",
    "vue-vine-tsc": "^0.1.5"
  }
}
