{
  "name": "vue-vine-playground",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "A vue-vine app created with create-vue-vine",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check only-build",
    "preview": "vite preview",
    "lint": "eslint src --cache",
    "lint:fix": "eslint src --cache --fix",
    "type-check": "vue-vine-tsc --noEmit --composite false",
    "only-build": "vite build"
  },
  "dependencies": {
    "pinia": "^3.0.2",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@tailwindcss/vite": "^4.1.7",
    "@types/node": "^22.15.19",
    "@vue-vine/eslint-config": "^1.1.0",
    "eslint": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue-vine": "^1.2.1",
    "vue-vine-tsc": "^0.1.5"
  },
  "onlyBuiltDependencies": [
    "@tailwindcss/oxide",
    "esbuild",
    "unrs-resolver"
  ]
}
